//conditionals
package video8;

public class Main {

	public static void main(String[] args) {
		int sayi = 26;
		if (sayi < 20) {
			System.out.println("Sayi 20'den kucuk");
		}else if(sayi == 20) {
			System.out.println("Sayi 20'ye esit");
		}else {
			System.out.println("Sayi 20'den kucuk degil");
		}
	}

}
